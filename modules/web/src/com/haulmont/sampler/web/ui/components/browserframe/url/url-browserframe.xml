<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd">
    <layout>
        <hbox height="100%"
              spacing="true"
              width="100%">
            <groupBox caption="BrowserFrame with a programmatically defined source"
                      height="100%">
                <browserFrame id="browserFrame"
                              height="100%"
                              width="100%"/>
            </groupBox>
            <groupBox caption="BrowserFrame with a declaratively defined source"
                      height="100%">
                <browserFrame height="100%"
                              width="100%">
                    <url url="https://doc.cuba-platform.com/manual-7.0/"/>
                </browserFrame>
            </groupBox>
        </hbox>
    </layout>
</window>
